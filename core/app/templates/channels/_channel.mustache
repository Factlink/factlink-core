<div id="channel">
  <header class="content-header channel-header">
    <h1 class="channel-title">
      {{ long_title }}
    </h1>

    {{#editable?}}
      <a href="{{ edit_link }}" class="edit">(edit)</a>
    {{/editable?}}

    <div class="add-to-channel-region"></div>

    <div class="pull-right">
      <div class="created_by_region"></div>
    </div>
  </header>

  <div class="js-subchannels-region channel-subchannels"></div>

  {{#global.can_haz.topic_facts}}
    <div>
      <ul>
        <li class="js-channel">Channel</span>
        <li class="js-topic">Topic</span>
      </ul>
    </div>
  {{/global.can_haz.topic_facts}}

  <div id="facts_for_channel"></div>
</div>
