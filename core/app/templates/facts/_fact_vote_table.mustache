      <button class="button vote-column-believes-button js-button-believes"><i class="icon-smile"></i></button>
    {{/global.signed_in}}

    {{^global.signed_in}}
      <span class="vote-table-cell-text-believes"><i class="icon-smile"></i></span>
    {{/global.signed_in}}
  </td>

  <td class="vote-column-believes-avatars js-avatars-believes-region"></td>
