  <td class="vote-column-disbelieves-avatars js-avatars-disbelieves-region"></td>

  <td class="vote-column-disbelieves js-cell-disbelieves">
    {{#global.signed_in}}
      <button class="button vote-column-disbelieves-button js-button-disbelieves"><i class="icon-frown"></i></button>
    {{/global.signed_in}}

    {{^global.signed_in}}
