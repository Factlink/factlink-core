<div class="js-user-heading-region"></div>
  <ul class="top-fact-share">
    <li class="top-fact-share-option"><a class="share-button discussion-share-button share-button-facebook" href="#">Facebook</a></li>
    <li class="top-fact-share-option"><a class="share-button discussion-share-button share-button-twitter" href="#">Twitter</a></li>
    {{#global.signed_in}}
      <li class="top-fact-share-option"><a class="share-button discussion-share-button share-button-message js-share" href="#">Share</a></li>
    {{/global.signed_in}}
  </ul>
<div class="top-fact-text">
  {{displaystring}}
</div>

<div class="top-fact-controls">

  <div class="top-fact-poster-controls">
    {{#showDelete}}
      <div class="top-fact-delete js-delete-region"></div>
    {{/showDelete}}
    <i class="icon-time"></i> {{created_by_ago}} by
    <span class="js-user-name-region"></span>
  </div>



  {{#global.signed_in}}
    <button class="button button-small button-confirm js-repost">Topics</button>
  {{/global.signed_in}}
</div>

<div class="top-fact-wheel">
  <div class="js-fact-wheel-region fact-wheel"></div>
</div>

<div class="top-fact-weakening-information">Weakening information</div>
<div class="top-fact-supporting-information">Supporting information</div>
