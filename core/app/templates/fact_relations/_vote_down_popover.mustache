{{#disbelieves_fact_relation}}
  <div>You think this is irrelevant. <button class="button button-small js-fact-relation-undisbelieve">Undo</button></div>
{{/disbelieves_fact_relation}}
{{^disbelieves_fact_relation}}
  <div>Is this Factlink irrelevant? <button class="button button-small js-fact-relation-disbelieve">Yes</button></div>
{{/disbelieves_fact_relation}}

{{#disbelieves_fact}}
  <div>You think this is incorrect or false. <button class="button button-small js-fact-undisbelieve">Undo</button></div>
{{/disbelieves_fact}}
{{^disbelieves_fact}}
  <div>Is this Factlink incorrect or false? <button class="button button-small js-fact-disbelieve">Yes</button></div>
{{/disbelieves_fact}}

<div class="tabular">
  <label class="tabular-row">
    <span class="tabular-cell">
      <input type="checkbox" class="js-fact-relation-disbelieve unbootstrapped-checkbox" {{#disbelieves_fact_relation}}checked="checked"{{/disbelieves_fact_relation}}>
    </span>
    <span class="tabular-cell">This argument is not relevant to the upper Factlink</span>
  </label>

  <label class="tabular-row">
    <span class="tabular-cell">
      <input type="checkbox" class="js-fact-disbelieve unbootstrapped-checkbox" {{#disbelieves_fact}}checked="checked"{{/disbelieves_fact}}>
    </span>
    <span class="tabular-cell">The Factlink in this argument is not correct or true.</span>
  </label>
</div>

<button class="js-cancel button">Cancel</button>
<button class="js-done button button-confirm">Done</button>
