<article class="list-block">
  <div class="user-avatar">
    {{{ avatar }}}
  </div>

  <div class="activity-container">
    <h1>{{ action }}</h1>
    <span class="body">
      <a href="{{ user_profile_url }}" rel="backbone">{{ username }}</a>
      created a {{ activity.channel_definition }}:
      <a href="{{ activity.channel_url }}">{{ activity.channel_title }}</a> </span>
    <span class="meta channel">{{{ activity.icon }}} {{ time_ago }}</span>
  </div>
  {{#debug}}
  <pre>:created_channel</pre>
  {{/debug}}
</article>

