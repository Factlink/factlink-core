<article class="fact">
  <div class="avatar">{{{ activity.avatar }}}</div>
	<header>
    <span class="body"><a href="{{ activity.user_profile_url }}">{{ activity.username }}</a> added <a href="{{ activity.channel_owner_profile_url }}">{{ activity.channel_owner }}'s</a> channel <a href="{{ activity.channel_url }}">{{ activity.channel_title }}</a> to his channels</span>
    <span class="meta channel">{{ activity.time_ago }}</span>
	</header>
</article>