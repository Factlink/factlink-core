<div class="left-col">
  <div class="user-avatar">
    {{{ avatar }}}
  </div>
</div>

<div class="right-col">
  <div>
    <strong><a href="{{ user_profile_url }}" class="heavy" rel="backbone">{{ username }}</a></strong>
    added
    <a href="{{ activity.channel_owner_profile_url }}" rel="backbone">{{ activity.channel_owner }}'s</a>
    {{ activity.channel_definition }}
    <a href="{{ activity.channel_url }}" rel="backbone">{{ activity.channel_title }}</a>
    to his {{ activity.channels_definition }}
  </div>

  <span class="meta">{{ time_ago }}</span>
</div>
