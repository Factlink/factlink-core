<div class="activity-group-user">
  <a href="{{ user.link }}" rel="backbone">
    <img alt=" " src="{{ user.avatar_url_24 }}" class="image-24px">
  </a>
</div>

<div class="activity-group-link">
  <a href="{{ user.link }}" rel="backbone">{{ user.name }}</a>
</div>

<div class="activity-group-time">{{ time_ago }} {{ global.t.ago }}</div>

<span class="activity-description">{{global.t.followed}}</span>

<span class="activity-inline-group-activities">
  <span class="js-region-activities separator-list">
    <span class="separator-list-item">
    <a href="{{followed_user.link}}" rel="backbone">{{ followed_user.name }}</a><span class="separator-list-sep"></span>
    </span>
  </span>
</span>


