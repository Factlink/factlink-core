<a href="{{ activity.target_url }}" target="_blank">
  <div class="avatar">
    <img alt=" " src="{{ user.avatar_url_24 }}" class="image-24px">
  </div>
  <div class="info">
    <div class="text">
      <span class="highlight">{{ user.name }}</span>
      added an argument to
      <span class="highlight">{{ activity.fact }}</span>
    </div>

    <div class="time_ago">
      <i class="icon-time time_ago_icon"></i>
      <span>{{time_ago}} {{ global.t.ago }}</span>
    </div>
  </div>
  <div class="clear"></div>
</a>
