<div class="avatar">
  {{{avatar}}}
</div>
<div class="info">
  <span><a href="{{ user_profile_url }}" rel="backbone">{{ username }}</a> added "{{ activity.evidence }}" as {{ activity.type }} evidence to "{{ activity.fact }}"</span>
  <div class="time_ago">
    <img src="/assets/time_ago.gif">
    <span>{{time_ago}}</span>
  </div>
</div>
<div class="clear"></div>