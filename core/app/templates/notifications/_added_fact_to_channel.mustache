<div class="avatar">
  {{{avatar}}}
</div>
<div class="info">
  <span>
    <a href="{{ activity.channel_url }}" rel="backbone">{{ username }}</a>
    added
    <a href="{{ activity.fact_url }}">"{{ activity.fact_displaystring }}"</a>
    to your channel
    <a href="{{ activity.channel_url }}"><strong>{{ activity.channel_title }}</strong></a>
  </span>
  <div class="time_ago">
    <img src="/assets/time_ago.gif">
    <span>{{time_ago}}</span>
  </div>
</div>
<div class="clear"></div>
