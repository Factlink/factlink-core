<div class="avatar">
  {{{avatar}}}
</div>
<div class="info">
  <span>
    <a href="{{ user_profile_url }}" rel="backbone">{{ username }}</a> added <a href="{{ activity.channel_owner_profile_url }}" rel="backbone">{{ activity.channel_owner }}'s</a> <a href="{{ activity.channel_url }}">{{ activity.channel_title }} channel</a> to his own channel <a href="{{activity.to_channel_url}}">{{activity.to_channel_title}}</a>
  </span>
  <div class="time_ago">
    <img src="/assets/time_ago.gif">
    <span>{{time_ago}}</span>
  </div>
</div>
<div class="clear"></div>