<div class="modal-layer js-feedback-modal">
  <div class="modal-window feedback-modal-window js-feedback-resize">
    <div class="modal-window-header">Provide us with feedback</div>
    <div class="modal-window-body">
      <iframe class="feedback-iframe" data-src="<%= feedback_index_path %>"<%= ' class="no-user"'.html_safe unless current_user %> scrolling="no"></iframe>
    </div>
  </div>
  <div class="modal-layer-close-background js-feedback-modal-close"></div>
</div>
