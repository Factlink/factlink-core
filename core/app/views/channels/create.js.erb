$("article.fact").each(function() {
  var $fact = $(this);
  console.log($fact);
  $fact.find(".add-to-channel").off('hover.load_channel_list');
  $fact.find(".add-to-channel").on('hover.load_channel_list',function(){
    console.log("calling something for" + $fact);
    $().factlink('getChannelChecklist',$fact);
  });
});

$().factlink('getChannelChecklist',$('article.fact[data-fact-id=<%=@fact.id%>]'));

if (Channels !== undefined && "<%=current_user.graph_user.id%>" === Channels.models[0].get('created_by_id') ) {
  Channels.add(<%= Channels::SingleMenuItem.for(channel: @channel,view: self).to_json.html_safe %>);
}
