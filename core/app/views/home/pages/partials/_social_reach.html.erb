<div class="publisher-container">
  <div class="publisher-animation-area">
    <div class="publisher-animation-social">
      <div class="publisher-animation-social-row">
        <div class="publisher-animation-social-column">
          <img src="<%= asset_path 'landing/content/Adrian.png' %>" class="animation-social-avatar">
        </div>
        <div class="publisher-animation-social-column">
          <img src="<%= asset_path 'landing/content/Blain.jpg' %>" class="animation-social-avatar">
        </div>
        <div class="publisher-animation-social-column">
          <img src="<%= asset_path 'landing/content/cdwharton-avatar.jpg' %>" class="animation-social-avatar">
        </div>
      </div>
      <div class="publisher-animation-social-row">
        <div class="publisher-animation-social-column">
          <img src="<%= asset_path 'landing/content/Innocenti.png' %>" class="animation-social-avatar">
        </div>
        <div class="publisher-animation-social-column">
          <div class="animation-social-primary-item">
            <div class="animation-social-icon">
              <div class="social-discuss-icon">
                <span class="social-discuss-icon-body"></span>
