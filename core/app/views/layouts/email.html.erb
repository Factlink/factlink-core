<html xmlns="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Factlink</title>
  </head>
  <body leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0" style="padding:0; width:100%; -webkit-text-size-adjust:none; margin:0; background-color:#EEEEEE" width="100%" bgcolor="#EEEEEE">
        <center>
            <table border="0" cellpadding="0" cellspacing="0" height="100%" width="100%" id="bodyTable" style="padding:0; width:100%; margin:0; background-color:#EEEEEE; height:100%" bgcolor="#EEEEEE"><tr><td align="center" valign="top" style="border-collapse:collapse">

                        <!-- // BEGIN CONTAINER -->
                        <table border="0" cellpadding="0" cellspacing="0" width="100%" id="templateContainer"><tr><td align="center" valign="top" style="border-collapse:collapse">
                                    <!-- // BEGIN BODY -->
                                    <table border="0" cellpadding="40" cellspacing="0" width="100%" id="templateBody" style="border-top:0; background-color:#EEEEEE; border-bottom:0" bgcolor="#EEEEEE"><tr><td align="center" valign="top" style="border-collapse:collapse">
                                                <table border="0" cellpadding="0" cellspacing="0" width="600"><tr><td align="center" valign="bottom" style="border-collapse:collapse">
                                                            <table border="0" cellpadding="40" cellspacing="0" width="600" id="bodyContentBlock" style="border-top:1px solid #DDDDDD; background-color:#FFFFFF; border-bottom:0" bgcolor="#FFFFFF"><tr><td align="left" valign="top" style="border-collapse:collapse; padding-top:40px">
                                                                        <table border="0" cellpadding="0" cellspacing="0"><tr><td style="font-size:20px; font-weight:bold; vertical-align:top; color:#505050; border-collapse:collapse; font-family:Helvetica; line-height:100%; text-align:left" align="left">
                                                                                    <img src="http://cdn.factlink.com/1/emailheader.png" style="outline:none; height:auto; max-width:520px; text-decoration:none; border:0; line-height:100%" id="campaign-icon" mc:label="header_image" mc:edit="header_image" mc:allowtext="" height="auto" /></td>
                                                                            </tr></table><table border="0" cellpadding="0" cellspacing="0" width="100%"><tr><td valign="top" style="font-size:15px; color:#505050; border-collapse:collapse; padding-top:20px; font-family:Helvetica; line-height:150%; text-align:left" mc:edit="body_content00" align="left">
<%= yield :email_body %>
<p style="float: right;"><em>&mdash; The Factlink team</em></p>
                                                                                </td>
                                                                            </tr></table></td>
                                                                </tr></table></td>
                                                    </tr><tr><td align="center" valign="top" style="border-collapse:collapse; padding-right:50px; padding-left:50px">
                                                            <img src="http://cdn.factlink.com/1/leaftshadow.png" height="auto" width="500" style="outline:none; display:block; height:auto; text-decoration:none; border:0; line-height:100%" /></td>
                                                    </tr></table></td>
                                        </tr></table><!-- END BODY \\ --></td>
                            </tr><tr><td align="center" valign="top" style="border-collapse:collapse">
                                </td>
                            </tr></table><!-- END CONTAINER \\ --></td>
                </tr></table></center>
