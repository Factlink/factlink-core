<li data-score="<%= factlink.total_score %>" id="fl-source-<%= factlink.id %>" class="<% if parent.supported_by?(factlink) %>supporting<% else %>weakening<% end %>">
	<div class="fl-source-stats">	 	
	 	<span class="fl-source-percentage">
	 	  12%
	 	</span>
	 	<sub class="fl-brain-count"><%= image_tag('/images/client/brains.png') %> 8080</sub>
	</div>
	
	<div class="fl-source-info">
	 	<!-- <div class="fl-vote-container">
  	   <% if user_signed_in? %>
  	     <%= vote_options(factlink, parent) %>
  	   <% end %>
  	 </div> -->

  	<h2 class="fl-source">&#8220;<%= factlink.displaystring %>&#8221;</h2>

  	<sub>
  		<span class="fl-source-created"><%= time_ago_in_words(factlink.created_at) %> ago by <%= link_to(factlink.created_by, user_profile_path(factlink.created_by.username), :target => "_blank") %></span>		
  		<% if factlink.site %>
  			<span class="fl-source-found">found on <a href="<%= factlink.site.url %>" target="_blank"><%= truncate(factlink.site.url, :length => 40) %></a></span>
  		<% else %>
  			<span class="fl-source-found">found on <a href="<%= factlink.url %>" target="_blank"><%= truncate(factlink.url, :length => 40) %></a></span>
  		<% end %>
  	</sub>

    <!-- <div class="fl-remove-fact">
      <%= link_to(image_tag('icons/users.png'), interacting_users_path(factlink), :remote => true, :onclick => "$('div#dimmer').fadeIn(); $('div#fl-interacting-users').fadeIn();") %>

      <% if user_signed_in? %>
        <% if factlink.added_to_parent_by_current_user(parent, current_user) %>
          <%= link_to(image_tag('trashcan.png'), remove_factlink_from_parent_path(factlink.id, parent.id), :remote => true, :confirm => "Are you sure you want to remove this supporting/weakening fact?") %>
        <% end %>
      <% end %>
    </div> -->

