<table cellpadding="0" cellspacing="0" border="0" class="added-argument-timeline">
  <tr><td class="timeline-left"></td><td class="timeline-right"></td></tr>
  <tr><td colspan="2" class="timeline-impact">
    <% if type == :supporting %>
      <div class="timeline-impact-indicator supporting">&nbsp;</div>
    <% else %>
      <div class="timeline-impact-indicator weakening">&nbsp;</div>
    <% end %>
  </td></tr>
  <tr><td class="timeline-left"></td><td class="timeline-right"></td></tr>
</table>
